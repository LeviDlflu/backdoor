<?xml version="1.0"?>
<Configuration>
  <SC-M22>
    <Selects>
      <SQL ID="SELECT_001">
        <Text> 
          SELECT 
            DISTINCT 管理ＮＯ種別 
          FROM 
            採番マスタ 
        </Text>
      </SQL>
      <SQL ID="SELECT_002">
        <Text>
          SELECT 
            管理ＮＯ種別,
            固定部,
            番号,
            変動データ部,
            備考 
          FROM 
            採番マスタ
        </Text>
      </SQL>
      <SQL ID="SELECT_003">
        <Text>
          SELECT 
            管理ＮＯ種別,
            固定部,
            番号,
            変動データ部,
            備考 
          FROM 
            採番マスタ 
          WHERE 
            管理ＮＯ種別 = '{0}'
        </Text>
      </SQL>
      <SQL ID="SELECT_004">
        <Text>
          SELECT 
            管理ＮＯ種別,
            固定部,
            番号,
            変動データ部,
            備考 
          FROM 
            採番マスタ 
          WHERE 
            管理ＮＯ種別 >= '{0}' 
          ORDER BY 
            管理ＮＯ種別,
            固定部
        </Text>
      </SQL>
      <SQL ID="SELECT_005">
        <Text>
          SELECT 
            管理ＮＯ種別 
          FROM 
            採番マスタ 
          WHERE 
            管理ＮＯ種別='{0}' 
          AND 
            固定部='{1}'
        </Text>
      </SQL>
    </Selects>
    
    <Updates>
      <SQL ID="UPDATE_001">
        <Text>
          UPDATE 
            採番マスタ 
          SET 
            変動データ部='{3}',
            番号={2},
            備考='{4}' 
          WHERE 
            管理ＮＯ種別='{0}' 
          AND 
            固定部='{1}'
        </Text>
      </SQL>
    </Updates>
    
    <Inserts>
      <SQL ID="INSERT_001">
        <Text>
          INSERT INTO  
            採番マスタ
              (
                管理ＮＯ種別,
                固定部,
                変動データ部,
                番号,
                備考
              ) 
              VALUES 
              (
                '{0}',
                '{1}',
                '{2}',
                '{3}',
                '{4}'
              )
        </Text>
      </SQL>
    </Inserts>
    
    <Deletes>
      <SQL ID="DELETE_001">
        <Text>
          DELETE FROM 
            採番マスタ 
          WHERE 
            管理ＮＯ種別='{0}' 
          AND 
            固定部='{1}'
        </Text>
      </SQL>
    </Deletes>
  </SC-M22>
</Configuration>