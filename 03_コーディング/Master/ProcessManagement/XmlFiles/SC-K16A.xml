<?xml version="1.0"?>
<Configuration>
  <SC-K16A>
    <Selects>
      
      <!--ビュー-->
      <SQL ID="SELECT_001">
        <Text>
          SELECT
          個体NO,
          判定時間 AS 判定日付・時間,
          数量,
          判定理由 AS 不良原因,
          氏名 AS 作業者,
          キャビ
          FROM
          K16A
          WHERE
          事業所コード='{0}'
          AND 大工程コード='10'
          AND 判定結果 = '{1}'
          {2}
        </Text>
      </SQL>
      
    </Selects>

    <!--条件-->
    <Wheres>
      <SQL ID="WHERE_001">
        <Text>
          AND 個体NO = '{0}'
        </Text>
      </SQL>
    </Wheres>
    
  </SC-K16A>
</Configuration>